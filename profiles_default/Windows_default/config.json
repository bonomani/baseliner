  "AdminApplyFirewallRules": {
    "firewallBatchOperations": [
      {
        "operation": "set-firewall-rule",
        "context": { "direction": "Inbound", "action": "Allow", "profiles": ["Private","Domain"] },
        "items": [
          {
            "name": "Allow RDP",
            "program": "%SystemRoot%\\System32\\svchost.exe",
            "service": "TermService",
            "port": 3389,
            "protocol": "TCP",
            "profiles": ["Private","Domain","Public"]
          },
          { "name": "vnc5800", "port": 5800, "protocol": "TCP" },
          { "name": "vnc5900", "port": 5900, "protocol": "TCP" },
          { "name": "Allow SSH", "port": 22, "protocol": "TCP" },
          { "name": "Allow ICMP Echo", "protocol": "ICMPv4", "direction": "Inbound", "action": "Allow", "profiles": ["Private","Domain","Public"] }
        ]
      }
    ]
  },
  
  
